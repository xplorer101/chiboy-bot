# OANDA API (Forex Trading)
# Get your API keys from: https://www.oanda.com/
OANDA_API_KEY=your_oanda_api_key_here
OANDA_ACCOUNT_ID=your_oanda_account_id_here
OANDA_ENV=practice  # Use 'practice' for demo, 'live' for real trading

# Binance API (Crypto Trading)
# Get your API keys from: https://www.binance.com/
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_SECRET_KEY=your_binance_secret_key_here
BINANCE_TESTNET=true  # Set to 'false' for live trading

# Trading Mode
DRY_RUN=true  # Set to 'false' to enable actual trade execution
EXECUTE_TRADES=false  # Only set to 'true' when you want to execute trades

# Logging
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR
